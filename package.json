{
  "name": "@jambudipa/spider",
  "version": "0.3.0",
  "description": "A comprehensive web scraping library with resumable operations, middleware support, and built-in rate limiting",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "vite build && tsc -p tsconfig.build.json --emitDeclarationOnly",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit --project tsconfig.typecheck.json",
    "typecheck:test": "tsc --noEmit --skipLibCheck --downlevelIteration --project tsconfig.test.json",
    "typecheck:examples": "tsc --noEmit --project tsconfig.examples.json",
    "prepublishOnly": "npm run clean && npm run build",
    "prepack": "npm run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "npm run build && changeset publish",
    "docs:api": "typedoc",
    "docs:build": "npm run docs:api",
    "examples:run": "tsx",
    "examples:run-all": "tsx examples/run-all.ts",
    "test:examples": "vitest run tests/examples.test.ts",
    "test:core": "vitest run tests/core",
    "test:all": "npm run test:examples && npm run test:core",
    "ci:validate": "tsx scripts/validate-ci.ts",
    "ci:local": "npm run ci:validate",
    "clean": "npx tsx ./scripts/clean.ts",
    "clean:verbose": "npx tsx ./scripts/clean.ts --verbose"
  },
  "keywords": [
    "web-scraping",
    "crawler",
    "spider",
    "http",
    "middleware",
    "resumable",
    "rate-limiting",
    "typescript"
  ],
  "author": "Jambudipa.io",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jambudipa/spider.git"
  },
  "bugs": {
    "url": "https://github.com/jambudipa/spider/issues"
  },
  "homepage": "https://github.com/jambudipa/spider#readme",
  "peerDependencies": {
    "cheerio": "^1.1.2",
    "domhandler": "^5.0.3",
    "effect": "^3.16.16",
    "tough-cookie": "^6.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.76",
    "@changesets/cli": "^2.29.5",
    "@playwright/test": "^1.54.2",
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^20.19.11",
    "@types/tough-cookie": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@typescript-eslint/typescript-estree": "^8.38.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/eslint-plugin": "^1.6.4",
    "@vitest/ui": "^1.6.0",
    "cheerio": "^1.1.2",
    "cli-progress": "^3.12.0",
    "commander": "^14.0.2",
    "domhandler": "^5.0.3",
    "effect": "^3.16.16",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsonc": "^2.21.0",
    "jsonc-eslint-parser": "^2.4.2",
    "pdf-parse": "^1.1.1",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "tough-cookie": "^6.0.0",
    "tslib": "^2.3.0",
    "tsx": "^4.20.3",
    "typedoc": "^0.28.9",
    "typedoc-plugin-markdown": "^4.8.0",
    "typescript": "^5.4.0",
    "typescript-eslint": "^8.51.0",
    "vite": "^5.0.0",
    "vitest": "^1.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {}
}
